											<Binding.Converter>
												<k:DoubleToThicknessConverter/>
											</Binding.Converter>
										</Binding>
									</Setter.Value>
								</Setter>
							</Trigger>
							<Trigger Property="VerticalScrollBarVisibility" Value="Disabled">
